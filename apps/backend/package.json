{
  "version": "0.1.0",
  "name": "@seats-sync/backend",
  "description": "Backend for the SeatsSync project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src -e ts --exec tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nanoid": "^5.0.8",
    "node-cache": "^5.1.2",
    "prisma": "^5.22.0",
    "speakeasy": "^2.0.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@seats-sync/config": "*",
    "@seats-sync/types": "*",
    "@seats-sync/constants": "*",
    "@seats-sync/utils": "*",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/speakeasy": "^2.0.10",
    "@types/ws": "^8.5.13",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "type-fest": "^4.36.0",
    "typescript": "^5.4.3",
    "eslint": "^9.22.0"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/lib": "dist/lib",
    "@/core": "dist/core",
    "@/constants": "dist/constants",
    "@/modules": "dist/modules",
    "@/shared": "dist/shared",
    "@/utils": "dist/utils"
  }
}
